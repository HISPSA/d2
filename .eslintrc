{
    "extends": "dhis2",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "class-methods-use-this": ["warn"]
    },
    "overrides": {
        "files": [ "**/*spec.js" ],
        "env": {
            "mocha": true,
            "node": true
        },
        "globals": {
            "expect": false,
            "sinon": false
        },
        "rules": {}
    }
}
